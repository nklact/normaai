{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Capability for mobile platforms (iOS/Android)",
  "platforms": ["iOS", "android"],
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "opener:allow-open-url",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-has",
    "store:allow-load",
    "store:allow-save",
    "web-auth:default",
    "web-auth:allow-authenticate",
    "machine-uid:default",
    "machine-uid:allow-get-machine-uid"
  ]
}
